<div class="card">
  <div class="card-body">

    <table id="datatable_plain" class="table table-bordered table-hover">
      <thead>
      <tr>
        <th>Harga</th>
        <th>Berlaku Mulai</th>
        <th></th>
      </tr>
      </thead>
      <tbody>
      <% fees.each do |fee| %>
        <tr>
          <td><%= number_to_currency(fee.amount) %></td>
          <td><%= fee.active_since %></td>
          <td></td>
        </tr>
      <% end %>
      </tbody>
    </table>

  </div>
<!-- ./card-body -->
</div>
<!-- ./card -->
